<Window x:Class="JPPInstaller.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JPPInstaller"
        mc:Ignorable="d"
        Title="JPP Installer" Height="450" Width="600"
        WindowStartupLocation="CenterScreen" 
        WindowState="Maximized">
    <ScrollViewer>
        <StackPanel>
            <Label FontFamily="Calibri" FontSize="24">Ironstone</Label>
            <WrapPanel Margin="0" Name="Products"/>
            <Label FontFamily="Calibri" FontSize="24">Cedar</Label>
            <WrapPanel Margin="0" Name="RevitProducts"/>
        </StackPanel>
    </ScrollViewer>
</Window>
